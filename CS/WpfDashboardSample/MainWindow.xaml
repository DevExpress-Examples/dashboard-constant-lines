<dx:ThemedWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:dxdash="http://schemas.devexpress.com/winfx/2008/xaml/dashboard" 
    xmlns:clExt="clr-namespace:ConstantLineExtension.WPF;assembly=ConstantLineExtension.WPF"
    x:Class="WpfDashboardSample.MainWindow"
    Title="MainWindow" Height="800" Width="1000">
    <Grid>
        <dxdash:DashboardControl Name="dashboardControl1">
            <dxmvvm:Interaction.Behaviors>
                <clExt:ConstantLineModule/>
            </dxmvvm:Interaction.Behaviors>
        </dxdash:DashboardControl>
    </Grid>
</dx:ThemedWindow>
